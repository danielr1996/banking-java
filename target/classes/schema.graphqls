scalar Date

type Query {
  buchungById(id: ID ): Buchung
  buchungen(page: Int, size: Int): [Buchung]
}

type Buchung{
  id: ID
  betrag: Float
  waehrung: String
  buchungstext: String
  verwendungszweck: String
  selfPartner: Transaktionspartner
  otherPartner: Transaktionspartner
  buchungstag: Date
  valutadatum: Date
}

type Transaktionspartner{
  id: ID
  iban: String
  bic: String
  name: String
}
